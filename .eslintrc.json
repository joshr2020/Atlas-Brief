{
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "env": {
        "browser": true
    },
    "extends": [
        "airbnb",
        "prettier"
    ],
    "plugins": [
        "jquery",
        "react"
    ],
    "rules": {
        "import/extensions": "always"
    }
}
